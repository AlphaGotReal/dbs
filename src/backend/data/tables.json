{
  "Agent": {
    "name": "Agent",
    "attributes_with_type": {
      "agent_id": "INT",
      "username": "VARCHAR(50)",
      "email": "VARCHAR(50)"
    },
    "keys": {
      "PRIMARY KEY (agent_id)": true
    }
  },

  "Property": {
    "name": "Property",
    "attributes_with_type": {
      "property_id": "INT",
      "property_type": "VARCHAR(50)",
      "address": "VARCHAR(50)",
      "city": "VARCHAR(50)",
      "state": "VARCHAR(50)",
      "zip_code": "VARCHAR(50)",
      "size": "FLOAT(10, 2)",
      "price": "FLOAT(10, 2)",
      "description": "VARCHAR(255)",
      "status": "BOOLEAN",
      "agent_id": "INT"
    },
    "keys": {
      "PRIMARY KEY (property_id)": true,
      "FOREIGN KEY (agent_id) REFERENCES Agent(agent_id)": true
    }
  },
  "Client": {
    "name": "Client",
    "attributes_with_type": {
      "client_id": "INT",
      "username": "VARCHAR(50)",
      "email": "VARCHAR(50)",
			"password": "VARCHAR(20)"
    },
    "keys": {
      "PRIMARY KEY (client_id)": true
    }
  },
  "Transaction": {
    "name": "Transaction",
    "attributes_with_type": {
      "transaction_id": "INT",
      "property_id": "INT",
      "client_id": "INT",
      "agent_id": "INT",
      "transaction_date": "DATE",
      "sale_price": "FLOAT(10, 2)"
    },
    "keys": {
      "PRIMARY KEY (transaction_id)": true,
      "FOREIGN KEY (property_id) REFERENCES Property(property_id)": true,
      "FOREIGN KEY (client_id) REFERENCES Client(client_id)": true,
      "FOREIGN KEY (agent_id) REFERENCES Agent(agent_id)": true
    }
  },
  "Image": {
    "name": "Image",
    "attributes_with_type": {
      "image_id": "INT",
      "property_id": "INT",
      "image_url": "VARCHAR(1000)"
    },
    "keys": {
      "PRIMARY KEY (image_id)": true,
      "FOREIGN KEY (property_id) REFERENCES Property(property_id)": true
    }
  },
  "PropertyFeatures": {
    "name": "PropertyFeatures",
    "attributes_with_type": {
      "feature_id": "INT",
      "property_id": "INT",
      "feature_name": "VARCHAR(50)"
    },
    "keys": {
      "PRIMARY KEY (feature_id)": true,
      "FOREIGN KEY (property_id) REFERENCES Property(property_id)": true
    }
  },
  "PropertyCategories": {
    "name": "PropertyCategories",
    "attributes_with_type": {
      "category_id": "INT",
      "category_name": "VARCHAR(50)",
      "property_id": "INT"
    },
    "keys": {
      "PRIMARY KEY (category_id)": true,
      "FOREIGN KEY (property_id) REFERENCES Property(property_id)": true
    }
  },
  "PropertyRatings": {
    "name": "PropertyRatings",
    "attributes_with_type": {
      "rating_id": "INT",
      "property_id": "INT",
      "client_id": "INT",
      "rating_value": "INT",
      "review": "TEXT",
      "date": "DATE"
    },
    "keys": {
      "PRIMARY KEY (rating_id)": true,
      "FOREIGN KEY (property_id) REFERENCES Property(property_id)": true,
      "FOREIGN KEY (client_id) REFERENCES Client(client_id)": true
    }
  }
}
